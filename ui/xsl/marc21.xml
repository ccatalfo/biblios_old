	<value name="RLen" position="0" length="5" inputtype="textbox">
		<description>The computer-generated, five-character numeric string that specifies the length of the entire record. The number is right justified and each unused position contains a zero.</description>
	</value>
	<value name="Base" position="12" length="5" inputtype="textbox">
	    <description>The computer-generated, five-character numeric string that indicates the first character position of the first variable control field in a record. The number is right justified and each unused position contains a zero.</description>
	</value>

